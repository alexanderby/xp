<?xml version="1.0" encoding="utf-8" ?>
<View xmlns="http://tempuri.org/Markup.xsd">
  <HBox flex="Stretch">
    <VBox width="50%">
      <Label text="UNDONE"/>
      <List items="{undone}" flex="Stretch">
        <Label text="{item.name}"/>
      </List>
    </VBox>
    <VBox width="50%">
      <Label text="DONE"/>
      <List items="{done}" flex="Stretch">
        <Label text="{item.name}"/>
      </List>
    </VBox>
  </HBox>
</View>
